addDefaultCharset UTF-8
<IfModule mod_autoindex.c>
Options -Indexes
</IfModule>

DirectoryIndex index.php index.html

<IfModule mod_rewrite.c>
Options +FollowSymlinks

RewriteEngine On
IndexIgnore */*
RewriteRule ^admin/(.*)?$ /backend/web/$1 [L,PT]
RewriteRule ^([^/].*)?$ /frontend/web/$1
</IfModule>